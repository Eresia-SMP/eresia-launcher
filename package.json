{
    "name": "eresia_smp_launcher_vitejs",
    "version": "0.1.0",
    "main": "dist/main/index.js",
    "license": "MIT",
    "scripts": {
        "build": "gulp && vite build",
        "watch:renderer": "vite",
        "watch:main": "concurrently \"gulp watch\" \"nodemon --watch dist/main --exec \\\"electron .\\\"\"",
        "dev": "yarn build && concurrently \"yarn watch:renderer\" \"yarn watch:main\""
    },
    "dependencies": {
        "electron": "^12.0.6",
        "electron-is-dev": "^2.0.0",
        "extract-zip": "^2.0.1",
        "lodash": "^4.17.21",
        "node-fetch": "^2.6.1",
        "sha1-file": "^2.0.0"
    },
    "devDependencies": {
        "@sveltejs/vite-plugin-svelte": "^1.0.0-next.9",
        "@types/extract-zip": "^2.0.1",
        "@types/lodash": "^4.14.170",
        "@types/node-fetch": "^2.5.10",
        "autoprefixer": "^10.2.6",
        "concurrently": "^6.2.0",
        "gulp": "^4.0.2",
        "gulp-typescript": "^6.0.0-alpha.1",
        "nodemon": "^2.0.7",
        "postcss": "^8.3.0",
        "prettier": "^2.3.0",
        "svelte": "^3.38.1",
        "svelte-preprocess": "^4.7.3",
        "tailwindcss": "^2.1.2",
        "typescript": "^4.3.2",
        "vite": "^2.2.4"
    }
